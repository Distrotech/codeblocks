<?xml version="1.0"?>
<!DOCTYPE CodeBlocks_lexer_properties>
<CodeBlocks_lexer_properties>
        <Lexer name="Tektronix extended HEX"
                index="119"
                filemasks="*.tek">
                <!--#define SCE_HEX_DEFAULT 0-->
                <Style name="Default"
                        index="0"
                        fg="0,0,0"
                        bg="255,255,255"
                        bold="0"
                        italics="0"
                        underlined="0"/>
                <!--#define SCE_HEX_RECSTART 1-->
                <Style name="Record start code"
                        index="1"
                        bg="255,255,204"/>

                <!--#define SCE_HEX_BYTECOUNT 3-->
                <Style name="Byte count field"
                        index="3"
                        bg="102,255,204"/>
                <!--#define SCE_HEX_BYTECOUNT_WRONG 4-->
                <Style name="Byte count field wrong"
                        index="4"
                        fg="255,0,0"
                        bg="102,255,204"/>

                <!--#define SCE_HEX_RECTYPE 2-->
                <Style name="Record type"
                        index="2"
                        bg="153,153,255"/>
                <!--#define SCE_HEX_RECTYPE_UNKNOW 17-->
                <Style name="Unknown record type"
                        index="17"
                        fg="255,0,0"
                        bg="153,153,255"/>

                <!--#define SCE_HEX_CHECKSUM 15-->
                <Style name="Checksum field"
                        index="15"
                        bg="255,153,153"/>
                <!--#define SCE_HEX_CHECKSUM_WRONG 16-->
                <Style name="Checksum field wrong"
                        index="16"
                        fg="255,0,0"
                        bg="255,153,153"/>


                <!--#define SCE_HEX_NOADDRESS 5-->
                <!--#define SCE_HEX_DATAADDRESS 6-->
                <Style name="Address field"
                        index="5,6"
                        bg="204,204,255"/>
                <!--#define SCE_HEX_ADDRESSFIELD_UNKNOWN 9-->
                <Style name="Unknown address field"
                        index="9"
                        fg="255,0,0"
                        bg="204,204,255"/>


                <!--#define SCE_HEX_DATA_ODD 11-->
                <!--#define SCE_HEX_DATA_EVEN 12-->
                <!--#define SCE_HEX_DATA_EMPTY 14-->
                <!--#define SCE_HEX_EXTENDEDADDRESS 10-->
                <!--#define SCE_HEX_STARTADDRESS 8-->
                <Style name="Data field"
                        index="11,12,14,10,8"
                        bg="153,255,255"/>
                <!--#define SCE_HEX_DATA_UNKNOWN 13-->
                <Style name="Data field unknown"
                        index="13"
                        fg="255,0,0"
                        bg="153,255,255"/>
                <!--#define SCE_HEX_GARBAGE 18-->
                <Style name="Garbage data"
                        index="18"
                        fg="255,0,0"/>

                <SampleCode value="lexer_thex.sample"/>
                <LanguageAttributes
                    LineComment=""
                    StreamCommentStart=""
                    StreamCommentEnd=""
                    BoxCommentStart=""
                    BoxCommentMid=""
                    BoxCommentEnd=""
                    CaseSensitive=""
                    LexerCommentStyles=""
                    LexerCharacterStyles=""
                    LexerStringStyles=""
                    LexerPreprocessorStyles=""/>
        </Lexer>
</CodeBlocks_lexer_properties>
